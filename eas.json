{
  "cli": {
    "version": ">= 14.0.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {}
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "REACT_APP_APIKEY": "@REACT_APP_APIKEY",
        "REACT_APP_AUTHDOMAIN": "@REACT_APP_AUTHDOMAIN",
        "REACT_APP_PROJECT_ID": "@REACT_APP_PROJECT_ID",
        "REACT_APP_STORAGEBUCKET": "@REACT_APP_STORAGEBUCKET",
        "REACT_APP_MESSAGINGSENDER_ID": "@REACT_APP_MESSAGINGSENDER_ID",
        "REACT_APP_APP_ID": "@REACT_APP_APP_ID",
        "REACT_APP_MEASUREMENT_ID": "@REACT_APP_MEASUREMENT_ID"
      }
    }
  },
  "submit": {
    "production": {}
  }
}